{
    "manifest_version": 3,
    "name": "QR Code Generator",
    "version": "1.0",
    "description": "Generate QR Codes quickly",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon128.png"
    },
    "permissions": [
      "activeTab"
    ],
    "icons": {
        "4": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png"
      }
  }
  